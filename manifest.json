{
  "manifest_version": 2,
  "name": "Vk blocker",
  "version": "1.0",
  
  "icons": {
    "128": ""
  },
  "content_scripts": [
    {
      "matches": [ "*://*/*" ],
      "js": [ "content.js" ]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "none"
  ],

      "browser_action": {
        "default_title": "Block vk!",
        "default_icon": "",
        "default_popup": "popup.html"
    }
	
}