{
  "manifest_version": 2,
  "name": "Vk blocker",
  "description" : "This is simple blocker for vk.com",
  "version": "1.0",


  "permissions": [
    "notifications"
  ],

  
  "icons": {
  },

  "options_page": "options.html",

  "background": {
    "scripts": ["background.js", "event.js"],
    "persistent": false
  },
  
      "browser_action": {
        "default_title": "Block vk!",
        "default_icon": "icon48.png", 
        "default_popup": "popup.html"
    }

	
}